<Window x:Class="Test5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Test5"
        mc:Ignorable="d"
        Title="Overwatch Stats Tracker" Height="475.022" Width="1007.149" Closing="Window_Closing" Background="#FF858484" ResizeMode="NoResize">


    <StackPanel Height="446" VerticalAlignment="Top">
        <Menu Background="DarkGray" FontFamily="/Overwatch Tracker;component/#BigNoodleTooOblique" FontSize="28">
            <MenuItem Header="File">
                <MenuItem Header="Save" Click="SaveCurrentSession_Click"/>
                <MenuItem Header="Save As" Click="SaveToFile_Button_Click"/>
                <MenuItem Header="Open" Click="LoadFromFile_Button_Click"/>
                <MenuItem Header="Export" Click="exportToExcel_Click"/>
                <MenuItem Header="Exit" Click="CloseApplication_Click"/>
            </MenuItem>
            <MenuItem Header="Edit">
                <MenuItem Header="Undo Last Log" Click="RemoveRow_Click"/>
            </MenuItem>
        </Menu>


        <Grid Background="#FF858484" Margin="0,0,2,0" Height="404">



            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="500*"/>
                <ColumnDefinition Width="67*"/>
                <ColumnDefinition Width="432*"/>

            </Grid.ColumnDefinitions>
            <StackPanel Grid.Column="0">

                <!--Match Setup-->
                <TextBlock Text="Match Setup" Margin="0 0 0 15" VerticalAlignment="Top" TextAlignment="Center" FontSize="42" FontFamily="/Overwatch Tracker;component/#BigNoodleTooOblique" FontWeight="Bold" Grid.Column="0"/>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <!--Text Side-->
                    </Grid.ColumnDefinitions>

                    <StackPanel Grid.Column="0">
                        <TextBlock HorizontalAlignment="Center" Margin="0 10 0 16"  Text="Role:"       FontSize="32" TextAlignment="Center" FontFamily="/Overwatch Tracker;component/Resources/#Koverwatch"/>
                        <TextBlock HorizontalAlignment="Center" Margin="0 10 0 16"  Text="Map Type:"   FontSize="32" TextAlignment="Center" FontFamily="/Overwatch Tracker;component/Resources/#Koverwatch"/>
                        <TextBlock HorizontalAlignment="Center" Margin="0 10 0 16"  Text="Map:"        FontSize="32" TextAlignment="Center" FontFamily="/Overwatch Tracker;component/Resources/#Koverwatch"/>
                        <TextBlock HorizontalAlignment="Center" Margin="0 10 0 16"  Text="Attacked:"   FontSize="32" TextAlignment="Center" FontFamily="/Overwatch Tracker;component/Resources/#Koverwatch"/>
                        <TextBlock HorizontalAlignment="Center" Margin="0 10 0 16"  Text="Win:"        FontSize="32" TextAlignment="Center" FontFamily="/Overwatch Tracker;component/Resources/#Koverwatch"/>

                    </StackPanel>


                    <!--Boxes-->
                    <StackPanel Grid.Column="1">


                        <ComboBox x:Name="roleComboBox" Margin="0 10 0 10" FontFamily="/Overwatch Tracker;component/Resources/#Koverwatch" VerticalContentAlignment="Center" FontSize="32" Grid.Column="1">
                            <ComboBoxItem Content="DPS"/>
                            <ComboBoxItem Content="Tank"/>
                            <ComboBoxItem Content="Heals"/>
                        </ComboBox>

                        <ComboBox x:Name="mapTypeComboBox" Margin="0 10 0 10" FontFamily="/Overwatch Tracker;component/Resources/#Koverwatch" FontSize="32" DropDownClosed="mapTypeComboBox_DropDownClosed" Grid.Column="1">
                            <ComboBoxItem Content="Hybrid"/>
                            <ComboBoxItem Content="Control"/>
                            <ComboBoxItem Content="Escort" />
                            <ComboBoxItem Content="Assault"/>
                        </ComboBox>

                        <ComboBox x:Name="mapComboBox" Margin="0 10 0 10" SelectedIndex="-1" FontFamily="/Overwatch Tracker;component/Resources/#Koverwatch" FontSize="32" Grid.Column="1"/>

                        <CheckBox x:Name="attackCheck" Margin="0 10 0 30" HorizontalAlignment="Left" BorderThickness="5"/>
                        <CheckBox x:Name="winCheckBox" Margin="0 10 0 10" HorizontalAlignment="Left" BorderThickness="5"/>

                        <Button x:Name="submitButton" Content="Log Match" HorizontalAlignment="Center" Click="submitButton_Click" FontSize="32"  FontFamily="/Overwatch Tracker;component/Resources/#Koverwatch" Width="167"/>

                    </StackPanel>


                    <!--Advanced-->
                    <StackPanel Grid.Column="2"/>

                </Grid>


            </StackPanel>

            <!-- Match Setup-->






            <TabControl Grid.Column="1" Grid.ColumnSpan="2" BorderBrush="Black" Background="#FFDEDEDE">
